{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"npmScope": "storybook-webpack-angular-types-plugin",
	"affected": {
		"defaultBase": "main"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx-cloud",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"],
				"accessToken": "ZmRhMDRhNDAtNzRlOS00Mzc5LTg0NjEtNjM0MTQ5NmMwOGU3fHJlYWQtd3JpdGU="
			}
		}
	},
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["production", "^production"]
		},
		"test": {
			"inputs": ["default", "^default", "{workspaceRoot}/jest.preset.js"]
		},
		"lint": {
			"inputs": ["default", "{workspaceRoot}/.eslintrc.json"]
		},
		"build-storybook": {
			"inputs": ["default", "^default", "{workspaceRoot}/.storybook/**/*"]
		}
	},
	"workspaceLayout": {
		"appsDir": "packages",
		"libsDir": "packages"
	},
	"generators": {
		"@nx/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nx/angular:library": {
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nx/angular:component": {
			"style": "scss"
		}
	},
	"defaultProject": "angular-demo",
	"namedInputs": {
		"default": ["{projectRoot}/**/*", "sharedGlobals"],
		"sharedGlobals": [],
		"production": ["default"]
	}
}
